<div class="container-fluid wrapper-left">
  <div class="row">
    <div class="col-lg-12"><h1 class="color-red">Dashboard</h1> <div class="breadcrumbs">Dashboard | Overview</div></div>
  </div>
</div>
<div class="container-fluid wrapper-stretch">
  <div class="row">
    <div class="col-lg-12">
      <div class="col-lg-12">
        <div class="col-lg-4 border-panel graph cpu">
          <div class="col-lg-12">
            <h3 style="margin-top: 0px; margin-bottom: 10px;">CPU</h3>
          </div>
          <div class="col-lg-12">
            <div id="line_div" style="width: 100%; height: 180px;"></div>
          </div>
        </div>
        <div class="col-lg-4 border-panel graph ram">
          <div class="col-lg-12">
            <h3 style="margin-top: 0px; margin-bottom: 10px;">RAM</h3>
            <h3 style="float: right; margin-top: -35px;">8 GB</h3>
          </div>
          <div class="col-lg-12">
            <div id="bar_div" style="width: 100%; height: 180px;"></div>
          </div>
        </div>
        <div class="col-lg-4 border-panel graph hdd">
          <div class="col-lg-12">
            <h3 style="margin-top: 0px; margin-bottom: 10px;">HDD</h3>
          </div>
          <div class="col-lg-12">
            <div id="pie_div" style="width: 100%; height: 180px;"></div>
          </div>
        </div>
      </div>
      <div class="col-lg-12">
        <div class="col-lg-6 border-panel graph network">
          <div class="col-lg-12">
            <h3 style="margin-top: 0px;">Network</h3>
          </div>
          <div class="col-lg-3">
            <h3 style="margin-top: 20px; margin-left: 0px; color: #000; font-weight: bold;">10 Mbps</h3><br/>
            <h5 style="margin-top: -15px; margin-left: 0px; color: #000;">Upload</h5>
            <h3 style="margin-top: 15px; margin-left: 0px; color: #000; font-weight: bold;">20 Mbps</h3><br/>
            <h5 style="margin-top: -15px; margin-left: 0px; color: #000;">Download</h5>
          </div>
          <div class="col-lg-9">
            <div id="chart_div" style="width: 100%; height: 180px;"></div>
          </div>
        </div>
        <div class="col-lg-4 border-panel graph monitor_status">
          <div class="col-lg-12">
            <h3 style="margin-top: 0px; margin-bottom: 10px;">Monitoring Status</h3>
          </div>
          <div class="col-lg-12">
            <dl style="width: 70%">
              <dt>CPU</dt>
              <dd><div id="data-one" class="bar" style="width: 60%">60%</div></dd>
              <dt>RAM</dt>
              <dd><div id="data-two" class="bar" style="width: 80%">80%</div></dd>
              <dt>HDD</dt>
              <dd><div id="data-three" class="bar" style="width: 64%">64%</div></dd>
              <dt>LAN</dt>
              <dd><div id="data-four" class="bar" style="width: 97%">97%</div></dd>
            </dl>
          </div>
        </div>
        <div class="col-lg-2 border-panel graph users">
          <div class="col-lg-12">
            <h3 style="margin-top: 0px; margin-bottom: 10px;">Clients</h3>
          </div>
          <div class="col-lg-12">
            <div class="glyphicon glyphicon-user" style="font-size: 5em; margin-left: 27px;"></div><br/><br/>
            <div class="glyphicon glyphicon-phone" style="font-size: 2.4em; margin-left: 1px; padding: 1px 1px;"></div>
            <div class="glyphicon glyphicon-home" style="font-size: 2.4em; padding: 2px 2px;"></div>
            <div class="glyphicon glyphicon-globe" style="font-size: 2.4em; padding: 2px 5px;"></div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- /#page-content-wrapper -->

</div>